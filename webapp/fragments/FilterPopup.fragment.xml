<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<ViewSettingsDialog id="viewSettingsDialog" title="Filter By" confirm="onConfirmViewSettingsDialog"
		resetFilters="onViewSettingsDialogResetFilters" filterItems="{/data}">
		<filterItems>
			<ViewSettingsFilterItem text="{title}" multiSelect="true" items="{items}">
				<items>
					<ViewSettingsItem text="{value}"/>
				</items>
			</ViewSettingsFilterItem>
		</filterItems>
	</ViewSettingsDialog>
</core:FragmentDefinition>